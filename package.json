{
  "name": "harmony-package",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/node": "^16.7.13",
    "@types/react-dom": "^19.0.0",
    "concurrently": "^9.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.4.2",
    "unzipper": "^0.12.3",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron .",
    "dev": "concurrently \"cross-env NODE_ENV=dev npm run start\" \"cross-env NODE_ENV=dev npm run electron\"",
    "packageWin": "electron-packager . oh-package --platform=win32 --arch=x64 --overwrite --electron-version=36.3.1 --out=build --icon=./public/logo.ico",
    "packageMac": "electron-packager . oh-package --platform=darwin --arch=x64 --overwrite --electron-version=36.3.1 --out=build --icon=./public/logo.ico"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^19.1.5",
    "cross-env": "^7.0.3",
    "electron": "^34.5.6",
    "electron-packager": "^17.1.2",
    "electron-reloader": "^1.2.3"
  },
  "description": "鸿蒙安装工具",
  "repository": "git@github.com:dongyeforever/oh-package.git",
  "license": "MIT"
}
